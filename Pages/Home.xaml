<Page
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPFUIKitProfessional.Pages"
      xmlns:Wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf" x:Class="WPFUIKitProfessional.Pages.Home"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Home" FontFamily="Poppins">

    <Border CornerRadius="8" Background="{DynamicResource SecundaryBackgroundColor}">
        <Grid>

            <Grid>

                <Wpf:WebView2
                  x:Name="webView" 
        NavigationStarting="webView_NavigationStarting" 
                  Margin="10,10,10,10" 
                  Source="https://sites.google.com/view/prototypea">

                </Wpf:WebView2>
            </Grid>
            <Button HorizontalAlignment="Left" Width="30" Height="30" VerticalAlignment="Top" Margin="21,-39,0,0" BorderBrush="{x:Null}" Click="Button_Click" Foreground="#FFBA0505">
                <Button.Background>
                    <ImageBrush ImageSource="/Pages/Photo/Back.png" Stretch="Uniform"/>
                </Button.Background>

            </Button>
        </Grid>


    </Border>
</Page>